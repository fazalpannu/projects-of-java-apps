<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imgimagandtext"
        android:layout_width="110dp"
        android:layout_height="119dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="48dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ImageContrastCheck"
        tools:srcCompat="@tools:sample/avatars"
        android:contentDescription="@string/hihkk" />

    <TextView
        android:id="@+id/txtoneimageandtext"
        android:layout_width="199dp"
        android:layout_height="34dp"
        android:layout_marginTop="72dp"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.477"
        app:layout_constraintStart_toEndOf="@+id/imgimagandtext"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtsecondimageandtext"
        android:layout_width="188dp"
        android:layout_height="38dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toEndOf="@+id/imgimagandtext"
        app:layout_constraintTop_toBottomOf="@+id/txtoneimageandtext" />
</androidx.constraintlayout.widget.ConstraintLayout>