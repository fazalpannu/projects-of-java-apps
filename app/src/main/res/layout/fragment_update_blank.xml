<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fitness"
    tools:context=".project.updateBlankFragment">

    <EditText
        android:id="@+id/namlog1"
        android:layout_width="243dp"
        android:layout_height="39dp"
        android:background="@color/white"
        android:ems="10"
        android:gravity="center"
        android:hint=" Enter  Name"
        android:inputType="textPersonName"
        android:textColor="@color/black"
        android:textColorHint="@color/purple_200"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/agelog1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id1"
        app:layout_constraintVertical_bias="0.5"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck,TextContrastCheck" />

    <EditText
        android:id="@+id/agelog1"
        android:layout_width="243dp"
        android:layout_height="39dp"
        android:background="@color/white"
        android:ems="10"
        android:gravity="center"
        android:hint="Enter Age"
        android:inputType="textPersonName"
        android:textColor="@color/black"
        android:textColorHint="@color/purple_200"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/weiglog1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/namlog1"
        app:layout_constraintVertical_bias="0.5"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck,TextContrastCheck" />

    <EditText
        android:id="@+id/heiglog1"
        android:layout_width="243dp"
        android:layout_height="39dp"
        android:background="@color/white"
        android:ems="10"
        android:gravity="center"
        android:hint="Enter Height"
        android:inputType="textPersonName"
        android:textColor="@color/black"
        android:textColorHint="@color/purple_200"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/button233"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weiglog1"
        app:layout_constraintVertical_bias="0.5"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck,TextContrastCheck" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="339dp"
        android:layout_height="74dp"
        android:gravity="center"
        android:text="ENTER    DETAILS For Update"
        android:textColor="#121111"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/id1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5"
        tools:ignore="HardcodedText,MissingConstraints,TextSizeCheck" />

    <EditText
        android:id="@+id/weiglog1"
        android:layout_width="243dp"
        android:layout_height="39dp"
        android:background="@color/white"
        android:ems="10"
        android:gravity="center"
        android:hint="Enter  Weight"
        android:inputType="textPersonName"
        android:textColor="@color/black"
        android:textColorHint="@color/purple_200"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/heiglog1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/agelog1"
        app:layout_constraintVertical_bias="0.5"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck,TextContrastCheck"
        tools:layout_editor_absoluteY="327dp" />

    <Button
        android:id="@+id/button233"
        android:layout_width="148dp"
        android:layout_height="57dp"
        android:background="@drawable/faz"
        android:text="Submit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/heiglog1"
        app:layout_constraintVertical_bias="0.5"
        tools:ignore="HardcodedText"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="521dp" />

    <EditText
        android:id="@+id/id1"
        android:layout_width="243dp"
        android:layout_height="39dp"
        android:background="@color/white"
        android:ems="10"
        android:gravity="center"
        android:hint="Enter  ID"

        android:textColor="@color/black"
        android:textColorHint="@color/purple_200"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/namlog1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView18"
        app:layout_constraintVertical_bias="0.5"
        tools:ignore="TextContrastCheck,TouchTargetSizeCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>